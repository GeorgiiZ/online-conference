{
  "name": "express-ts",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "ts:build": "tsc",
    "build": "npm run ts:build",
    "dev:serve": "set DEBUG=app, app:* & nodemon src/app.ts",
